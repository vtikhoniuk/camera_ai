{
  "expo": {
    "name": "CameraAIApp",
    "slug": "CameraAIApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.CameraAIApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow camera access to take photos.",
          "microphonePermission": "Allow microphone access to record video."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow access to photos to save images.",
          "savePhotosPermission": "Allow saving photos to gallery."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4e890824-5b07-417f-aa5a-4f128ae31a2a"
      }
    }
  }
}
